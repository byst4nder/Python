            $(page.lazyCssArr).each(function () {
                var link = document.createElement('link');
                link.href = this;
                link.rel = 'stylesheet';
                link.type = 'text/css';
                vw.container.appendChild(link);
            });
        }
    };
    if (page.fsDocRenderDone) {
        loadCss();
    }
    else {
        $mediator.on('docRenderDone', function (e, data) {
            var ops = typeof data === 'object' ? data : {};
            ops.isFirstAll && loadCss();
        });
